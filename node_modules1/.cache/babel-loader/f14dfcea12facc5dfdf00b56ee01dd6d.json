A,CAAA","sourcesContent":["<template>\r\n  <div class=\"home\">\r\n      <div v-for=\"(item,index) in lis\" :key=\"index\" :id=\"item.letter\" class=\"box\" >\r\n          <p class=\"tit\">{{item.letter}}</p>\r\n              <ul class=\"ull\">\r\n                <li v-for=\"(item,index) in item.arr\" :key=\"index\" class=\"lii\" @click=\"rleft(item.MasterID)\">\r\n                    <img v-lazy=\"item.CoverPhoto\" alt=\"\">\r\n                    <span>{{item.Name}}</span>   \r\n                </li>\r\n              </ul> \r\n      </div>\r\n      <ul class=\"right\">\r\n        <li v-for=\"(item,index) in lis\" :key=\"index\" >\r\n            <p @click='fun(item.letter)'>\r\n              {{item.letter}}\r\n            </p> \r\n        </li>\r\n      </ul>\r\n\r\n        <van-popup v-model=\"flage\" \r\n                 position=\"right\" \r\n                 :style=\"{ height: '100%',width:'70%' }\" \r\n                 :overlay-style=\"{background:'none'}\"\t\r\n                 > \r\n        <Rleft class=\"rl\" />\r\n      </van-popup>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport {mapActions, mapState} from 'vuex'\r\nimport Rleft from '../components/Rleft'\r\nimport { black } from 'color-name';   \r\nexport default {\r\n  data(){\r\n    return {\r\n      flage:false\r\n    }\r\n  },\r\n  name: 'home',\r\n  components: {\r\n      Rleft,\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      list: state=>state.home.list,\r\n      lis: state=>state.home.lis,\r\n      right:state=> state.home.right\r\n    })\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      getMasterBrandList:'home/getMasterBrandList',\r\n      getMasterRightList: 'home/getMasterRightList'\r\n    }),\r\n    rleft(MasterID){\r\n      this.flage=!this.flage\r\n      this.getMasterRightList(MasterID)\r\n      if(document.querySelector('.lii')){\r\n         document.querySelector('.rl').style='display:block'\r\n      } \r\n    },\r\n     fun(item){\r\n          document.querySelector('.home').scrollTop = document.querySelector(`#${item}`).offsetTop;\r\n      }\r\n  },\r\n  created() {\r\n    this.getMasterBrandList();\r\n    this.getMasterRightList();\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .rl{\r\n    width:5.6rem ;\r\n    height: 100%;\r\n    background: white;\r\n    position: fixed;\r\n    right: 0;\r\n    top: 0;\r\n  }\r\n  .home{\r\n    width: 100%;\r\n    height: 100%;\r\n    overflow-y: scroll;\r\n    background-color:none\r\n  }\r\n  .box {\r\n    width: 100%;\r\n  }\r\n  .ull{\r\n    width: 6.9rem;\r\n    text-align: center;\r\n    background-color:none\r\n  }\r\n  .right{\r\n  position: fixed;\r\n  right: 0px;\r\n  top: 120px;\r\n  p{\r\n    line-height: 20px;\r\n    font-size: 12px;\r\n    color: #666666;\r\n  }\r\n }\r\n  .lii{\r\n    width: 100%;\r\n    height: 1rem;\r\n    display: flex;\r\n    margin-left: 15px;\r\n    border-bottom: 1px solid #ddd;\r\n    background-color:none\r\n  }\r\n  img{\r\n    width: .8rem;\r\n    height: .8rem;\r\n  }\r\n  span {\r\n  font-size: .32rem;\r\n  margin-left: .4rem;\r\n  line-height: 1rem;\r\n  }\r\n  .tit{\r\n    font-size: .28rem;\r\n    line-height: .4rem;\r\n    background: #f4f4f4;\r\n    padding-left: .3rem;\r\n    color: #aaa;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}