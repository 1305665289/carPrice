ll from './better-scroll/scroll';\r\nimport CommonList from \"./CommonList\"\r\nexport default {\r\n    computed: {\r\n            ...mapState({\r\n                count: state => state.pic.count,\r\n                value: state => state.pic.imageList,\r\n                page: state => state.pic.page\r\n            }),\r\n             pullDownRefreshObj: () => {\r\n                return {\r\n                    threshold: 90,\r\n                    stop: 50,\r\n                    txt: '刷新成功'\r\n                }\r\n            },\r\n            pullUpLoadObj: () => {\r\n                return {\r\n                    threshold: 50,\r\n                    txt: {\r\n                        more: '加载更多',\r\n                        noMore: '没有更多数据了'\r\n                    }\r\n                }\r\n            }\r\n        },\r\n            watch: {\r\n            pullDownRefreshObj: {\r\n                handler(val) {\r\n                    const scroll = this.$refs.scroll.scroll\r\n                    if (val) {\r\n                        scroll.openPullDown()\r\n                    } else {\r\n                        scroll.closePullDown()\r\n                    }\r\n                },\r\n                deep: true\r\n            },\r\n            pullUpLoadObj: {\r\n                handler(val) {\r\n                    const scroll = this.$refs.scroll.scroll\r\n                    if (val) {\r\n                        scroll.openPullUp()\r\n                    } else {\r\n                        scroll.closePullUp()\r\n                    }\r\n                },\r\n                deep: true\r\n            },\r\n        },\r\n        //注册组件\r\n        components:{\r\n          CommonList,\r\n          Scroll\r\n        },\r\n        methods:{\r\n              ...mapActions({\r\n                getImageTypeList: 'pic/getImageTypeList',\r\n                refreshDispatch: 'pic/getImageTypeList',\r\n                loadMoreDispatch: 'pic/getImageTypeList'\r\n         